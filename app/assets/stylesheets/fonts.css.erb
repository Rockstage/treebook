@font-face {
  font-family: "Century Gothic";
  src: url('<%= asset_path('GOTHIC.eot') %>');
  src: url('<%= asset_path('GOTHIC.eot') + '?#iefix' %>') format('eot'), url('<%= asset_path('GOTHIC.woff') %>') format('woff'), url('<%= asset_path('GOTHIC.ttf') %>') format('truetype'), url('<%= asset_path('GOTHIC.svg') + '#GOTHIC' %>') format('svg');
  font-weight: normal;
  font-style: normal;
}